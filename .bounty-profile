#---------------------------------------
bounty-tools(){
    echo "01 - virtualhost"
    echo "02 - subdomain"
    echo "03 - cert-subdomain"
    echo "04 - bucket"
    echo "05 - screenshot"
    echo "06 - github"
    echo "07 - scanner"
    echo "08 - link-scanner"
    echo "09 - takeover"
    echo "10 - aws-cli"
    echo "11 - cert-subdomain"
}


#---------------------------------------
bt(){
  cd ~/zk-scripts/bounty-tools
  pull -i
  src
}

bp(){
  cd ~/zk-scripts/bounty-profile
  pull
  src
}



#----- misc -----

hgrip(){ 
    history | grep $1
}

ipinfo(){
    curl http://ipinfo.io/$1
}



#------ Tools ------
burp(){
    cd ~/tools/Burpsuite
	java -jar burp-loader-keygen.jar
	cd ~
}


ncx(){
    nc -l -n -vv -p $1 -k
}