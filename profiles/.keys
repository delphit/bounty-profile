keys(){
    
    GREEN=$(tput setaf 2)
    NC=$(tput sgr0)
    
    echo "${GREEN}AWS Configuraion${NC}"
    echo "aws configure"
    echo -e "\n"

    echo "${GREEN}FB_APP_TOKEN for Facebook API${NC}"
    echo "export FB_TOKEN=x01x02x03"
    echo -e "\n"

    echo "${GREEN}Knock VirusTotal Key${NC}"
    echo "knock/knockpy/config.json"
    echo -e "\n"

    echo "${GREEN}Subfinder${NC}"
    echo "subfinder --set-config VirustotalAPIKey=x01x02x03"
    echo "subfinder --set-config PassivetotalUsername=hacker,PassivetotalKey=supersecret"
    echo "subfinder --set-config SecurityTrailsKey=x01x02x03"
    echo "subfinder --set-config RiddlerEmail=hacker,RiddlerPassword=x01x02x03"
    echo "subfinder --set-config CensysUsername=API_ID,CensysSecret=x01x02x03"
    echo "subfinder --set-config ShodanAPIKey=x01x02x03"
    echo -e "\n"

    echo "${GREEN}Amass will have api-key functionality in later version${NC}"
    echo -e "\n"

    echo "${GREEN}Gitrob${NC}"
    echo "export GITROB_ACCESS_TOKEN=x01x02x03"
    echo -e "\n"

    echo "${GREEN}GitAllSecrets${NC}"
    echo "export GITHUB_ACCESS_TOKEN=x01x02x03"
    echo -e "\n"

    echo "${GREEN}Bucketstream${NC}"
    echo "bucket-stream/config.yaml"
    echo -e "\n"

    echo "${GREEN}CloudFlair${NC}"
    export CENSYS_API_ID=x01x02x03
    export CENSYS_API_SECRET=x01x02x03
    echo -e "\n"


}